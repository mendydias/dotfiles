{
  "Latex Fraction": {
    "prefix": ["fr"],
    "body": ["\\frac{$1}{$2}$0"]
  },
  "greater than or equal": {
    "prefix": [">="],
    "body": ["\\geq $0"]
  },
  "less than or equal": {
    "prefix": ["<="],
    "body": ["\\leq $0"]
  },
  "square": {
    "prefix": ["sr"],
    "body": ["^{2} "]
  },
  "power": {
    "prefix": ["^"],
    "body": ["$0^{$1}"]
  },
  "subfix": {
    "prefix": ["_"],
    "body": ["$0_{$1}"]
  },
  "verbatim": {
    "prefix": ["txt", "text"],
    "body": ["\\text{$1}"]
  },
  "basic braces": {
    "prefix": ["bb"],
    "body": ["\\{ $0 \\\\}"]
  },
  "basic parentheses": {
    "prefix": ["bp"],
    "body": ["($1)"]
  },
  "basic square brackets": {
    "prefix": ["bs"],
    "body": ["[ $1 ]"]
  },
  "large braces": {
    "prefix": ["lb"],
    "body": ["\\bigl\\{ $1 \\\\bigr\\\\}"]
  },
  "large parentheses": {
    "prefix": ["lp"],
    "body": ["\\left( $1 \\\\right)"]
  },
  "large square brackets": {
    "prefix": ["ls"],
    "body": ["\\bigl[ $1 \\\\bigr]"]
  },
  "log": {
    "prefix": ["log"],
    "body": ["\\log_{$1} $2"]
  },
  "sum": {
    "prefix": ["sum"],
    "body": ["\\sum_{$1}^{$2} $3"]
  },
  "inline": {
    "prefix": ["mk"],
    "body": ["$$"]
  }
}
